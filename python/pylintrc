[MESSAGES CONTROL]
disable=missing-docstring,
        too-many-arguments,
        too-many-instance-attributes,
        too-many-locals,
        too-many-branches,
        too-few-public-methods,
        invalid-name

[BASIC]
good-names=i,j,k,ex,Run,_,x,y,id,db,f

[MASTER]
extension-pkg-whitelist=lxml
