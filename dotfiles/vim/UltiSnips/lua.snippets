snippet fun
function ${1:fname}(${2:...})
	${0:-- body}
end
endsnippet

snippet for
for ${1:i} = ${2:1}, ${3:10} do
	${0:print(i)}
end
endsnippet

snippet forp
for ${1:i}, ${2:v} in pairs(${3:table_name}) do
	${0:-- body}
end
endsnippet

snippet fori
for ${1:i}, ${2:v} in ipairs(${3:table_name}) do
	${0:-- body}
end
endsnippet
