<tskel:before>
let b:tskelBibID = input("ID of bibentry: ")
if b:tskelBibID == "" | let b:tskelBibID = "<+CURSOR+>" | endif
</tskel:before>
<tskel:after>
unlet b:tskelBibID
</tskel:after>
@INCOLLECTION{<+b:tskelBibID+>,
    author       = {<+CURSOR+>},
    title        = {<+BIB TITLE+>},
    crossref     = {<+CROSSREF+>},
    pages        = {<+PAGES+>},
    chapter      = {<+CHAPTER+>},
    <+bit:standardEntries+>
}
<++>
